    "prettier": "prettier src/**/*.{js,jsx,ts,tsx}",
    "lint": "eslint . --ext tsx",
    "checkup": "yarn prettier --check && yarn lint",
    "format": "yarn prettier --write && yarn lint --fix",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "deploy": "PATH_PREFIX=/web gatsby build --prefix-paths && gh-pages -d public -r https://$GH_TOKEN@github.com/agenciaglobal/web.git"
